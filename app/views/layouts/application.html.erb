<!DOCTYPE html>
<html>
  <head>
    <title>TweetAppRails</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
    <% if current_user %>
     <%= button_to "Sign Out", log_out_path, method: :get ,  class: 'signout'  %>
    <% end %>
  <body>
    <%= yield %>
  </body>
</html>
